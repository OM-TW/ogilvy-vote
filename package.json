{
  "name": "ogilvy-vote",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:serve": "concurrently \"npm run dev\" \"npm run serve\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "rm": "rm -rf node_modules/ dist/ package-lock.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "deploy": "NODE_ENV=production netlify deploy --dir=dist",
    "deploy:prod": "NODE_ENV=production netlify deploy --dir=dist --prod",
    "serve": "NODE_ENV=local netlify dev --dir=dist"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "lesca-fetcher": "^1.1.6",
    "lesca-local-storage": "^1.1.2",
    "lesca-number": "^1.5.5",
    "lesca-react-onload": "^1.0.14",
    "lesca-use-countdown": "^1.0.8",
    "lesca-use-tween": "^3.0.6",
    "lesca-user-agent": "^2.0.1",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.5",
    "react-dom": "^18.2.0",
    "react-loading": "^2.0.3",
    "react-router-dom": "^6.19.0",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@netlify/functions": "^2.4.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "daisyui": "^4.4.2",
    "devcert": "^1.2.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "express": "^4.18.2",
    "gh-pages": "^6.1.0",
    "less": "^4.2.0",
    "mongoose": "^8.0.1",
    "msw": "^2.0.8",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-html": "^3.2.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
